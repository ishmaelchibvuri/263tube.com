{
  "name": "debt-payoff-planner-backend",
  "version": "1.0.0",
  "description": "Backend Lambda functions for Debt Payoff SA",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && cdk deploy",
    "seed:exams": "ts-node scripts/seed-exams.ts",
    "seed:re5": "ts-node scripts/seed-re5-exam.ts",
    "seed:exam-sets": "ts-node scripts/seed-exam-simulation-sets.ts",
    "seed": "npm run seed:exams",
    "cleanup-db": "ts-node scripts/cleanup-database.ts",
    "import-re5": "ts-node scripts/import-re5-questions.ts",
    "reset-db": "npm run cleanup-db && npm run import-re5"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.700.0",
    "@aws-sdk/client-dynamodb": "^3.932.0",
    "@aws-sdk/client-ses": "^3.700.0",
    "@aws-sdk/client-ssm": "^3.940.0",
    "@aws-sdk/lib-dynamodb": "^3.932.0",
    "@aws-sdk/util-dynamodb": "^3.932.0",
    "@getbrevo/brevo": "^3.0.1",
    "aws-jwt-verify": "^5.1.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.13.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "uuid": "^9.0.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.10.1",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
